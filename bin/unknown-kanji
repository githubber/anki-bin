#!/usr/bin/env perl
use utf8::all;
use 5.16.0;
use warnings;
use Anki::Database;

my $anki = Anki::Database->new;

my %seen;

$anki->each_card(sub {
    my ($card) = @_;
    return unless $card->model->name eq '漢字';
    $seen{ $card->field('漢字') }++;
});

$seen{'々'}++;
$seen{'〇'}++;

while (<>) {
    my $listed = 0;
    for my $kanji (/\p{Han}/g) {
        if (!$seen{$kanji}) {
            print $kanji;
            $listed++;
        }
    }
    print "\n" if $listed;
}
